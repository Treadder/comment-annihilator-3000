{
  "name": "comment-annihilator-3000",
  "displayName": "Comment Annihilator 3000",
  "description": "Remove comments fast, and without mercy",
  "version": "0.0.4",
  "publisher": "JaredKelnhofer",
  "engines": {
    "vscode": "^1.104.0" 
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Treadder/comment-annihilator-3000"
  },
  "activationEvents": [
    "onCommand:comment-annihilator-3000.removeComments"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "comment-annihilator-3000.removeComments",
        "title": "Annihilate Comments"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "echo 'No compilation needed for pure JS'",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "eslint": "^8.0.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
